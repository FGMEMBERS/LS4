            <button>0</button>
            <repeatable>false</repeatable>
                <binding>
                    <command>property-assign</command>
                    <property>/sim/hitches/hook-open</property>
                    <value type="bool">true</value>
                </binding>
                <binding>
                    <command>property-assign</command>
                    <property>/sim/hitches/aerotow/open</property>
                    <value type="bool">true</value>
                </binding>
                <binding>
                    <command>property-assign</command>
                    <property>/sim/hitches/winch/open</property>
                    <value type="bool">true</value>
                </binding>
                <mod-up>
                    <binding>
                        <command>property-assign</command>
                        <property>/sim/hitches/hook-open</property>
                        <value type="bool">false</value>
                    </binding>
                </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>hooks</tooltip-id>
                <label>Hooks: %s</label>
                <mapping>open-close</mapping>
                <property>/sim/hitches/hook-open</property>
            </binding>
        </hovered>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>hook-handle</object-name>
        <property>/sim/hitches/hook-open</property>
        <factor>0.02</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <!--Canopy Jettison Handle-->
    <animation>
        <type>pick</type>
        <object-name>jettison-handle</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <condition>
                    <equals>
                    <property>sim/model/door-positions/canopyFJ/position-norm</property>
                    <value>0</value>
                </equals>
                </condition>
                <command>property-assign</command>
                <property>controls/canopy/lock-jettison</property>
                <value>1</value>
            </binding>
            <binding>
                <condition>
                    <and>
                        <equals>
                            <property>sim/model/door-positions/canopyFJ/position-norm</property>
                            <value>0</value>
                        </equals>
                        <greater-than>
                            <property>/velocities/airspeed-kt</property>
                            <value>30</value>
                        </greater-than>
                    </and>
                </condition>
                <command>nasal</command>
                <script>doors.canopyFJ.toggle();</script>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property>controls/canopy/lock-jettison</property>
                    <value>0</value>
                </binding>
            </mod-up>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>canopyjettison</tooltip-id>
                <label>Canopy JETTISON LOCK: %s</label>
                <mapping>open-close</mapping>
                <property>controls/canopy/lock-jettison</property>
            </binding>
        </hovered>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>jettison-handle</object-name>
        <property>controls/canopy/lock-jettison</property>
        <factor>0.02</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
