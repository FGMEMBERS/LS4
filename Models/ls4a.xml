<PropertyList>

 <path>ls4a.ac</path>

  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/LS4/Models/Liveries", 10);

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      
      var canopy_door = self.getNode("sim/model/door-positions/canopy/position-norm", 1);
      var canopyFJ_door = self.getNode("sim/model/door-positions/canopyFJ/position-norm", 1);
      
      canopy_door.alias(float_0);
      canopyFJ_door.alias(float_1);
    </load>

    <unload>
      livery_update.stop();
      canopy_door.unalias();
      canopyFJ_door.unalias();
    </unload>
  </nasal>
 
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>canopy</object-name>
  </effect>
 
  
 <!--Canopy-->
  <animation>
    <type>pick</type>
     <object-name>canopy</object-name>
     <object-name>canopy.int</object-name>
     <object-name>canopyframe</object-name>
     <object-name>door.int</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.canopy.toggle();</script>
      </binding>
    </action>
  </animation>
 
